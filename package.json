    "@types/react-dom": "19.2.2",
    "postcss": "8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "5.7.3",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "class-variance-authority": "0.7.1",
